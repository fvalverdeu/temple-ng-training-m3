{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "libs",
  "projects": {
    "idstore-web": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/idstore-web",
      "sourceRoot": "apps/idstore-web/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/idstore-web",
            "index": "apps/idstore-web/src/index.html",
            "main": "apps/idstore-web/src/main.ts",
            "polyfills": "apps/idstore-web/src/polyfills.ts",
            "tsConfig": "apps/idstore-web/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/idstore-web/src/favicon.ico",
              "apps/idstore-web/src/assets"
            ],
            "styles": [
              "apps/idstore-web/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/idstore-web/src/environments/environment.ts",
                  "with": "apps/idstore-web/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "idstore-web:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "idstore-web:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "idstore-web:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/idstore-web/src/test.ts",
            "polyfills": "apps/idstore-web/src/polyfills.ts",
            "tsConfig": "apps/idstore-web/tsconfig.spec.json",
            "karmaConfig": "apps/idstore-web/karma.conf.js",
            "assets": [
              "apps/idstore-web/src/favicon.ico",
              "apps/idstore-web/src/assets"
            ],
            "styles": [
              "apps/idstore-web/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/idstore-web/tsconfig.app.json",
              "apps/idstore-web/tsconfig.spec.json",
              "apps/idstore-web/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "apps/idstore-web/e2e/protractor.conf.js",
            "devServerTarget": "idstore-web:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "idstore-web:serve:production"
            }
          }
        }
      }
    },
